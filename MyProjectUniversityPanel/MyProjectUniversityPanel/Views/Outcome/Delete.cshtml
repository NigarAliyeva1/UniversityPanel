@model Outcome
<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Are you sure to delete </h4>
                <p class="card-description">"@Model.For" outcome?</p>
                <form class="forms-sample" method="post">

                    <button type="submit" class="btn btn-gradient-primary me-2">Yes</button>
                    <a asp-action="Index" class="btn btn-light">Cancel</a>
                </form>
            </div>
        </div>
    </div>
</div>
